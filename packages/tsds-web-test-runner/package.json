{
  "name": "tsds-web-test-runner",
  "version": "1.14.0",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./createConfig.cjs": "./dist/cjs/exports/createConfig.js",
    "./createConfig.mjs": "./dist/esm/exports/createConfig.mjs"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.cjs",
    "format": "biome check --write --unsafe src/",
    "test": "",
    "test:engines": "npm test",
    "test2": "tsds test:node --no-timeouts",
    "version": ""
  },
  "dependencies": {
    "@web/dev-server-esbuild": "^1.0.3",
    "@web/dev-server-import-maps": "^0.2.1",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-playwright": "^0.11.0",
    "assert": "1.5.0",
    "cross-spawn-cb": "^2.1.14",
    "getopts-compat": "^2.2.6",
    "install-optional": "^0.1.34",
    "link-unlink": "^0.1.18",
    "queue-cb": "^1.4.13",
    "resolve-bin-sync": "^0.1.8",
    "tsds-lib": "*",
    "webpack-merge": "^6.0.1"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}

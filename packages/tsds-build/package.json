{
  "name": "tsds-build",
  "version": "1.13.19",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "mocha",
    "rollup",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs",
    "types": "./dist/types/index.d.ts"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.cjs",
    "format": "biome check --write --unsafe src/",
    "test": "",
    "test2": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "cross-spawn-cb": "^2.1.6",
    "get-tsconfig-compat": "^2.0.3",
    "install-optional": "^0.1.8",
    "lodash.camelcase": "4.3.0",
    "queue-cb": "^1.4.12",
    "resolve-bin-sync": "^0.1.6",
    "rimraf2": "^2.8.2",
    "rollup": "^4.30.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "ts-swc-rollup-plugin": "^1.10.9",
    "ts-swc-transform": "^1.15.8",
    "tsds-lib": "^1.13.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.6",
    "rolldown": "^1.0.0-beta.1",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}

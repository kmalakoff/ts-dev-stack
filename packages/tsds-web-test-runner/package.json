{
  "name": "tsds-web-test-runner",
  "version": "1.17.4",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./createConfig.cjs": "./dist/cjs/exports/createConfig.js",
    "./createConfig.mjs": "./dist/esm/exports/createConfig.js"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.cts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../scripts/build.cjs",
    "format": "biome check --write --unsafe src/",
    "test": "",
    "test2": "mocha --no-timeouts test/**/*.test.*",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "@web/dev-server-esbuild": "*",
    "@web/dev-server-import-maps": "*",
    "@web/test-runner": "*",
    "@web/test-runner-playwright": "*",
    "assert": "1.5.0",
    "cross-spawn-cb": "*",
    "getopts-compat": "*",
    "install-optional": "*",
    "link-unlink": "*",
    "lodash.debounce": "*",
    "queue-cb": "*",
    "resolve-bin-sync": "*",
    "tsds-lib": "*",
    "webpack-merge": "*"
  },
  "devDependencies": {
    "@types/mocha": "*",
    "@types/node": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}

{
  "name": "ts-dev-stack",
  "version": "1.7.9",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "mocha",
    "rollup",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "bin": {
    "tsds": "bin/cli.cjs"
  },
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.mjs",
    "depcheck": "depcheck",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "cross-spawn-cb": "^1.5.7",
    "depcheck": "^1.4.7",
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "gh-pages": "^6.2.0",
    "install-module-linked": "^0.2.0",
    "lazy-cache": "^2.0.2",
    "link-unlink": "^0.1.5",
    "queue-cb": "^1.4.4",
    "resolve": "^1.22.10",
    "sort-package-json": "^2.12.0",
    "tsds-biome": "^1.7.7",
    "tsds-build": "^1.7.7",
    "tsds-c8": "^1.7.9",
    "tsds-lib": "^1.7.7",
    "tsds-mocha": "^1.7.9",
    "tsds-typedoc": "^1.7.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "module-link-unlink": "^0.1.3",
    "os-shim": "^0.1.3",
    "short-hash": "^1.0.0",
    "tsds-lib-test": "^1.6.15",
    "tsds-web-test-runner": "^1.7.7",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "tsds-web-test-runner": "^1.4.12"
  },
  "peerDependenciesMeta": {
    "tsds-web-test-runner": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=0.8"
  },
  "gitHead": "5d936c167b89e3ac836c29d08eba26fa0fab3b92",
  "tsds": {
    "source": "src/index.ts"
  }
}

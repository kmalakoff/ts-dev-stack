{
  "name": "ts-dev-stack",
  "version": "1.11.21",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "mocha",
    "rollup",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "bin": {
    "tsds": "bin/cli.cjs"
  },
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.mjs",
    "clean": "node -e \"const dir = require('path').join(require('os').homedir(), '.im'); try {require('fs').rmdirSync(dir, {recursive: true})} catch(err) {}\"",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "cross-spawn-cb": "^2.0.13",
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "gh-pages": "^6.3.0",
    "install-module-linked": "^1.0.7",
    "link-unlink": "^0.1.9",
    "np": "^10.1.0",
    "queue-cb": "^1.4.9",
    "resolve": "^1.22.10",
    "resolve-bin-sync": "^0.1.3",
    "sort-package-json": "^2.12.0",
    "tsds-biome": "^1.11.21",
    "tsds-build": "^1.11.21",
    "tsds-c8": "^1.11.21",
    "tsds-lib": "^1.11.21",
    "tsds-mocha": "^1.11.21",
    "tsds-typedoc": "^1.11.21",
    "tsds-web-test-runner": "^1.11.21"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "module-link-unlink": "^0.1.10",
    "os-shim": "^0.1.3",
    "short-hash": "^1.0.0",
    "tsds-lib-test": "^1.11.21",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "gitHead": "6351d2f9fe10282485467d05036a55315bd6d9d9",
  "tsds": {
    "source": "src/index.ts"
  }
}

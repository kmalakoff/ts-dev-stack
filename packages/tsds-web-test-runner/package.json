{
  "name": "tsds-web-test-runner",
  "version": "1.18.1",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "stack",
    "test",
    "typescript"
  ],
  "homepage": "https://github.com/kmalakoff/ts-dev-stack",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./createConfig.cjs": "./dist/cjs/exports/createConfig.js",
    "./createConfig.mjs": "./dist/esm/exports/createConfig.js"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "@web/dev-server-esbuild": "*",
    "@web/dev-server-import-maps": "*",
    "@web/test-runner": "*",
    "@web/test-runner-playwright": "*",
    "assert": "1.5.0",
    "cross-spawn-cb": "*",
    "getopts-compat": "*",
    "install-optional": "*",
    "link-unlink": "*",
    "lodash.debounce": "*",
    "queue-cb": "*",
    "resolve-bin-sync": "*",
    "tsds-lib": "*",
    "webpack-merge": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "mocha": "*",
    "mocha-compat": "*",
    "module-link-unlink": "*",
    "node-version-use": "*",
    "os-shim": "*",
    "queue-cb": "*",
    "resolve": "*",
    "short-hash": "*",
    "ts-dev-stack": "*",
    "ts-swc-loaders": "*",
    "tsds-lib-test": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}

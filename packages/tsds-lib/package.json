{
  "name": "tsds-lib",
  "version": "1.0.5",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../tsds-build/scripts/build.js",
    "depcheck": "depcheck",
    "format": "biome check --write --unsafe src/ test/",
    "test": "",
    "test2": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^1.1.2",
    "env-path-key": "^1.0.0",
    "mkdirp": "0.5.6",
    "path-string-prepend": "^1.0.0",
    "queue-cb": "^1.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.1",
    "depcheck": "^1.4.7",
    "rollup": "^4.28.0",
    "tsds-lib-test": "file:../tsds-lib-test",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}

{
  "name": "tsds-publish",
  "version": "1.0.2",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "mocha",
    "rollup",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/tsds-publish.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs",
    "types": "./dist/types/index.d.ts"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.cjs",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "@npmcli/arborist": "^9.0.0",
    "cross-spawn-cb": "^2.1.2",
    "get-remote": "^1.3.10",
    "getopts-compat": "^2.2.6",
    "np": "^10.1.0",
    "npm-package-arg": "^12.0.1",
    "pacote": "^21.0.0",
    "queue-cb": "^1.4.10",
    "tsds-validate": "^1.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "module-link-unlink": "^0.1.12",
    "os-shim": "^0.1.3",
    "resolve": "^1.22.10",
    "short-hash": "^1.0.0",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}

{
	"$schema": "https://biomejs.dev/schemas/2.0.0/schema.json",
	"extends": ["./biome.json"],
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"complexity": {
				"useArrowFunction": "off"
			},
			"suspicious": {
        "noVar": "off"
			},
			"style": {
        "useTemplate": "off"
			}
		}
	}
}
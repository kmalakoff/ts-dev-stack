{
  "name": "tsds-build",
  "version": "1.8.1",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "mocha",
    "rollup",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "tsds build",
    "depcheck": "depcheck",
    "format": "biome check --write --unsafe src/",
    "test": "",
    "test2": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "cross-spawn-cb": "^1.5.7",
    "get-tsconfig-compat": "^2.0.2",
    "lodash.camelcase": "4.3.0",
    "module-root-sync": "^0.1.1",
    "queue-cb": "^1.4.4",
    "rimraf2": "^2.8.2",
    "rollup": "^4.29.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "ts-swc-rollup-plugin": "^1.6.5",
    "ts-swc-transform": "^1.9.2",
    "tsds-lib": "^1.8.1",
    "tsds-typedoc": "^1.8.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "rolldown": "^1.0.0-beta.1",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  },
  "gitHead": "5d936c167b89e3ac836c29d08eba26fa0fab3b92"
}

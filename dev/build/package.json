    "tsds-lib": "^1.11.22"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "cross-spawn-cb": "^2.0.13",
    "queue-cb": "^1.4.10",
    "rimraf2": "^2.8.2",
    "rolldown": "^1.0.0-beta.1"
  }

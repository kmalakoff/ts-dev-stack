{
  "name": "tsds-mocha",
  "version": "1.11.17",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "dev",
    "development",
    "mocha",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.mjs",
    "docs": "tsds docs",
    "format": "biome check --write --unsafe src/ && biome check --config-path biome.es5.json --write --unsafe scripts/",
    "postinstall": "node ./scripts/postinstall.cjs",
    "test": "",
    "test2": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "assert": "1.5.0",
    "cross-spawn-cb": "^2.0.6",
    "getopts-compat": "^2.2.5",
    "link-unlink": "^0.1.7",
    "mocha": "^11.0.1",
    "mocha-compat": "^3.6.2",
    "queue-cb": "^1.4.7",
    "regenerator-runtime": "^0.14.1",
    "resolve": "^1.22.10",
    "resolve-bin-sync": "^0.1.2",
    "ts-swc-loaders": "^1.10.5",
    "tsds-lib": "^1.11.17",
    "unixify": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  },
  "gitHead": "fe6e20748ee8ca42fcd143445c0dab1f06710a86"
}

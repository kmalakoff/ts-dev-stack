{
  "name": "ts-dev-stack",
  "version": "0.8.6",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "eslint",
    "mocha",
    "prettier",
    "rollup",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "bin": {
    "tsds": "./bin/ts-dev-stack.js"
  },
  "files": [
    "lib",
    "config"
  ],
  "scripts": {
    "build": "",
    "predeploy": "npm run lint && npm run format && depcheck --config .config/.depcheckrc.json && sort-package-json",
    "deploy": "np --no-yarn",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "mocha-compat test/spec/**/*.test.js --no-timeouts",
    "test:engines": "npm test"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@swc/core": "1.2.224",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "assert": "^1.5.0",
    "c8": "^7.12.0",
    "call-once-fn": "^1.0.1",
    "cross-spawn-cb": "^0.6.14",
    "depcheck": "^1.4.3",
    "env-path-key": "^0.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-mocha": "^10.1.0",
    "exit": "^0.1.2",
    "fs-access-compat": "^1.0.3",
    "fs-iterator": "^4.1.1",
    "getopts-compat": "^2.2.5",
    "gh-pages": "^4.0.0",
    "just-extend": "^6.0.1",
    "karma": "^6.4.0",
    "karma-chrome-launcher": "^3.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-webpack": "^5.0.0",
    "lodash.camelcase": "^4.3.0",
    "mkdirp": "^0.5.6",
    "mocha": "^10.0.0",
    "mocha-compat": "^3.5.5",
    "np": "^7.6.2",
    "os-shim": "^0.1.3",
    "path-string-prepend": "^0.2.1",
    "prettier": "^2.7.1",
    "queue-cb": "^1.1.7",
    "regenerator-runtime": "^0.13.9",
    "rimraf": "^2.7.1",
    "rollup": "2.76.0",
    "rollup-plugin-node-externals": "^3.1.2",
    "rollup-plugin-terser": "^7.0.2",
    "short-hash": "^1.0.0",
    "sort-package-json": "^1.57.0",
    "ts-constants": "^0.1.0",
    "ts-loader": "^9.3.1",
    "ts-node": "10.8.2",
    "ts-swc-loaders": "^0.0.2",
    "typedoc": "^0.23.10",
    "typedoc-plugin-rename-defaults": "^0.6.4",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0"
  },
  "engines": {
    "node": ">=0.8"
  }
}

{
  "name": "tsds-mocha",
  "version": "1.7.6",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "dev",
    "development",
    "mocha",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.mjs",
    "depcheck": "depcheck",
    "format": "biome check --write --unsafe src/ scripts/",
    "postinstall": "node ./scripts/postinstall.cjs",
    "test": "",
    "test2": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "assert": "1.5.0",
    "getopts-compat": "^2.2.5",
    "link-unlink": "^0.1.5",
    "mocha": "^10.8.2",
    "mocha-compat": "^3.6.2",
    "queue-cb": "^1.4.4",
    "regenerator-runtime": "^0.14.1",
    "ts-swc-loaders": "^1.6.1",
    "tsds-lib": "^1.7.6",
    "unixify": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "depcheck": "^1.4.7",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  },
  "gitHead": "5d936c167b89e3ac836c29d08eba26fa0fab3b92"
}

{
  "env": {
    "esm": {
      "presets": [
        ["@babel/preset-react", { "flow": false, "typescript": true }],
        ["@babel/typescript", { "isTSX": true, "allExtensions": true }],
        ["@babel/env", { "modules": false, "targets": { "node": "10" } }]
      ],
      "plugins": ["add-import-extension"]
    },
    "cjs": {
      "presets": [
        ["@babel/preset-react", { "flow": false, "typescript": true }],
        ["@babel/typescript", { "isTSX": true, "allExtensions": true }],
        ["@babel/env", { "modules": "cjs" }]
      ],
      "plugins": ["add-module-exports"]
    },
    "umd": {
      "presets": [["@babel/preset-react", { "flow": false, "typescript": true }], ["@babel/typescript", { "isTSX": true, "allExtensions": true }], "@babel/env"]
    },
    "test-legacy": {
      "presets": [
        ["@babel/preset-react", { "flow": false, "typescript": true }],
        ["@babel/typescript", { "isTSX": true, "allExtensions": true }],
        ["@babel/env", { "modules": "cjs" }]
      ],
      "ignore": ["**/*.d.ts"]
    }
  }
}

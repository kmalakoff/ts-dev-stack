{
  "name": "tsds-web-test-runner",
  "version": "1.7.1",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*",
    "./createConfig.cjs": "./dist/cjs/exports/createConfig.js",
    "./createConfig.mjs": "./dist/esm/exports/createConfig.mjs"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.mjs",
    "depcheck": "depcheck",
    "format": "biome check --write --unsafe src/",
    "test": "",
    "test2": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "@web/dev-server-esbuild": "^1.0.3",
    "@web/dev-server-import-maps": "^0.2.1",
    "@web/test-runner": "^0.19.0",
    "@web/test-runner-playwright": "^0.11.0",
    "assert": "1.5.0",
    "getopts-compat": "^2.2.5",
    "link-unlink": "^0.1.5",
    "queue-cb": "^1.4.4",
    "tsds-lib": "^1.7.1",
    "webpack-merge": "^6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "depcheck": "^1.4.7",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  },
  "gitHead": "5d936c167b89e3ac836c29d08eba26fa0fab3b92"
}

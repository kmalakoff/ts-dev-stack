{
  "name": "ts-dev-stack",
  "version": "1.1.0",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "c8",
    "dev",
    "development",
    "mocha",
    "rollup",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "bin": {
    "tsds": "bin/cli.js"
  },
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../tsds-build/scripts/build.js",
    "depcheck": "depcheck",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "cross-spawn-cb": "^1.1.2",
    "depcheck": "^1.4.7",
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "gh-pages": "^6.2.0",
    "node-version-use": "^1.0.2",
    "queue-cb": "^1.2.1",
    "resolve": "^1.22.8",
    "sort-package-json": "^2.12.0",
    "ts-swc-loaders": "^1.0.4",
    "tsds-biome": "file:../tsds-biome",
    "tsds-build": "file:../tsds-build",
    "tsds-c8": "file:../tsds-c8",
    "tsds-lib": "file:../tsds-lib",
    "tsds-mocha": "file:../tsds-mocha",
    "tsds-typedoc": "file:../tsds-typedoc",
    "tsds-web-test-runner": "file:../tsds-web-test-runner"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.1",
    "tsds-lib-test": "file:../tsds-lib-test",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.mjs",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}

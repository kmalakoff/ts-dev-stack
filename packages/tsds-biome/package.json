{
  "name": "tsds-biome",
  "version": "1.4.1",
  "description": "Development stack for TypeScript libraries",
  "keywords": [
    "dev",
    "development",
    "mocha",
    "stack",
    "test",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-dev-stack.git"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "bin": {
    "tsds-biome": "bin/cli.cjs"
  },
  "files": [
    "dist",
    "assets",
    "scripts"
  ],
  "scripts": {
    "build": "node ../../dev/build/scripts/build.mjs",
    "depcheck": "depcheck",
    "format": "biome check --write --unsafe src/",
    "test": "",
    "test2": "tsds test:node --no-timeouts",
    "test:engines": "npm test",
    "version": ""
  },
  "dependencies": {
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "node-version-use": "^1.0.4",
    "resolve": "^1.22.10",
    "tsds-lib": "^1.4.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "depcheck": "^1.4.7",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts",
    "targets": [
      "cjs",
      "esm"
    ]
  }
}
